.side-bar {
	width: 227px;
	height: 100vh;
	background-color: #303F9F;
}

.side-bar__container {
	width: 92%;
	height: 100%;
	margin: 0 auto;
}

.side-bar__logo-img {
	fill: none;
	stroke: #fff;
	stroke-width: 3;
	stroke-linecap: round;
	stroke-miterlimit: 10;
	filter: drop-shadow(1px 2px 4px rgba(0, 0, 0, 0.25));
}

.side-bar__list-itschool {
	transform: translate(0.000000,161.000000) scale(0.100000,-0.100000);
	fill: #000000;
	stroke: none;
}
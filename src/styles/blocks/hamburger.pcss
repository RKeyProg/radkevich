.hamburger {
    width: 30px;
    height: 25px;
    display: none;
    flex-direction: column;
    justify-content: space-between;

    @include tablets {
        display: flex;
    }
}

.humburger__line {
    display: block;
    background-color: $text-color_light;
    width: 100%;
    height: 5px;
}